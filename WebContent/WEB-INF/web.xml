  <filter>
    <filter-name>UrlMap</filter-name>
    <filter-class>net.kukido.servlet.filter.UrlMapFilter</filter-class>
    <!-- JSON Resources -->
    <init-param>
      <param-name>pattern1</param-name>
      <param-value>.*/json/maps/(.+.)$</param-value>
    </init-param>
    <init-param>
      <param-name>template1</param-name>
      <param-value>/JsonGpsTrack.do?fileName=$1</param-value>
    </init-param> 
    <!-- Main -->
    <init-param>
      <param-name>pattern2</param-name>
      <param-value>.*/home/?</param-value>
    </init-param>
    <init-param>
      <param-name>template2</param-name>
      <param-value>/LogEntryListing.do?pageSize=5</param-value>
    </init-param>
    <init-param>
      <param-name>pattern3</param-name>
      <param-value>.*/home/?(\d*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template3</param-name>
      <param-value>/LogEntryListing.do?page=$1&amp;pageSize=5</param-value>
    </init-param>
    <!-- Archives (more at template ~19) -->
    <init-param>
      <param-name>pattern4</param-name>
      <param-value>.*/archive/(\d+)</param-value>
    </init-param>
    <init-param>
      <param-name>template4</param-name>
      <param-value>/ViewLogEntry.do?entryId=$1</param-value>
    </init-param>
    <!-- Sections -->
    <init-param>
      <param-name>pattern5</param-name>
      <param-value>.*/sections/([^/]+)/?(\d*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template5</param-name>
      <param-value>/LogEntryListing.do?section=$1&amp;page=$2</param-value>
    </init-param>
    <!-- Topics -->
    <init-param>
      <param-name>pattern6</param-name>
      <param-value>.*/topics/([^/]+)/?(\d*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template6</param-name>
      <param-value>/SearchLogEntries.do?topic=$1&amp;page=$2</param-value>
    </init-param>
    <!-- Quick Links -->
    <init-param>
      <param-name>pattern7</param-name>
      <param-value>.*/quicklinks/?(\d*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template7</param-name>
      <param-value>/QuickLinkListing.do?page=$1</param-value>
    </init-param>
    <!-- RSS -->
    <init-param>
      <param-name>pattern8</param-name>
      <param-value>.*/monkeyfeed.xml$</param-value>
    </init-param>
    <init-param>
      <param-name>template8</param-name>
      <param-value>/Monkeyfeed.do</param-value>
    </init-param>
    <init-param>
      <param-name>pattern9</param-name>
      <param-value>.*/feeds/([^/]+).xml</param-value>
    </init-param>
    <init-param>
      <param-name>template9</param-name>
      <param-value>/Monkeyfeed.do?tags=$1</param-value>
    </init-param>
    <init-param>
      <param-name>pattern10</param-name>
      <param-value>.*/feeds/rss2html.xslt</param-value>
    </init-param>
    <init-param>
      <param-name>template10</param-name>
      <param-value>/jsp/pages/Rss2Html.jsp</param-value>
    </init-param>
    <!-- Loggin in -->
    <init-param>
      <param-name>pattern11</param-name>
      <param-value>.*/login$</param-value>
    </init-param>
    <init-param>
      <param-name>template11</param-name>
      <param-value>/Login.do</param-value>
    </init-param>
    <!-- Attachments -->
    <init-param>
      <param-name>pattern12</param-name>
      <param-value>.*/attachments/([^/]*)</param-value>
    </init-param>
    <init-param>
      <param-name>template12</param-name>
      <param-value>/ViewAttachment.do?fileName=$1</param-value>
    </init-param>
    <init-param>
      <param-name>pattern13</param-name>
      <param-value>.*/attachments/thumbs/(.*)</param-value>
    </init-param>
    <init-param>
      <param-name>template13</param-name>
      <param-value>/ViewAttachmentResize.do?fileName=$1&amp;maxDimension=75</param-value>
    </init-param>
    <init-param>
      <param-name>pattern14</param-name>
      <param-value>.*/attachments/postcards/(.*)</param-value>
    </init-param>
    <init-param>
      <param-name>template14</param-name>
      <!-- Note that setting the maxDimension here will NOT affect the size of map thumnails.  For that, see MapImage JSP tag impl. -->
      <param-value>/ViewAttachmentResize.do?fileName=$1&amp;maxDimension=426</param-value>
    </init-param>
    <!-- Galleries -->
    <init-param>
      <param-name>pattern15</param-name>
      <param-value>.*/galleries/(.*)</param-value>
    </init-param>
    <init-param>
      <param-name>template15</param-name>
      <param-value>/Gallery.do?attachmentId=$1</param-value>
    </init-param>
    <!-- Mopho Entries -->
    <init-param>
      <param-name>pattern16</param-name>
      <param-value>.*/mopho/(\d+)$</param-value>
    </init-param>
    <init-param>
      <param-name>template16</param-name>
      <param-value>/MophoEntryListing.do?page=$1</param-value>
    </init-param>
    <init-param>
      <param-name>pattern17</param-name>
      <param-value>.*/mopho/?$</param-value>
    </init-param>
    <init-param>
      <param-name>template17</param-name>
      <param-value>/MophoEntryListing.do</param-value>
    </init-param>
    <init-param>
      <param-name>pattern18</param-name>
        <param-value>.*/mopho/(.+)</param-value>
    </init-param>
    <init-param>
      <param-name>template18</param-name>
      <param-value>/ViewMophoImage.do?fileName=$1</param-value>
    </init-param>
    <!-- About Me -->
    <init-param>
      <param-name>pattern19</param-name>
      <param-value>.*/about/?</param-value>
    </init-param>
    <init-param>
      <param-name>template19</param-name>
      <param-value>/WhatsAllThis.do</param-value>
    </init-param>
    <!-- Archives by Date -->
    <init-param>
      <param-name>pattern20</param-name>
      <param-value>.*/archive/bydate/(\d+)/?(\d*)/?(\d*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template20</param-name>
      <param-value>/SearchLogEntries.do?year=$1&amp;month=$2&amp;date=$3</param-value>
    </init-param>
    <!-- Archives by Tag -->
    <init-param>
      <param-name>pattern21</param-name>
      <param-value>.*/tags/([^/]*)(/(\d*))?</param-value>
    </init-param>
    <init-param>
      <param-name>template21</param-name>
      <param-value>/SearchLogEntries.do?tags=$1&amp;pageSize=30&amp;page=$3</param-value>
    </init-param>
    <!-- Searching -->
    <init-param>
      <param-name>pattern22</param-name>
      <param-value>.*/search</param-value>
    </init-param>
    <init-param>
      <param-name>template22</param-name>
      <param-value>/SearchLogEntries.do</param-value>
    </init-param>
    <!-- Comment Feeds -->
    <init-param>
      <param-name>pattern23</param-name>
      <param-value>.*/feeds/comments/(\d+).xml</param-value>
    </init-param>
    <init-param>
      <param-name>template23</param-name>
      <param-value>/CommentRss.do?entryId=$1</param-value>
    </init-param>
    <init-param>
      <param-name>pattern24</param-name>
      <param-value>.*/commentspam.xml</param-value>
    </init-param>
    <init-param>
      <param-name>template24</param-name>
      <param-value>/CommentRss.do?spam=true</param-value>
    </init-param>
    <init-param>
      <param-name>pattern25</param-name>
      <param-value>.*/recentcommentsnav</param-value>
    </init-param>
    <init-param>
      <param-name>template25</param-name>
      <param-value>/RecentCommentsNav.do</param-value>
    </init-param>
    <init-param>
      <param-name>pattern26</param-name>
      <param-value>.*/comments.xml</param-value>
    </init-param>
    <init-param>
      <param-name>template26</param-name>
      <param-value>/RecentCommentsFeed.do?numComments=10</param-value>
    </init-param>
    <!-- GPX to Google Mapping -->
    <init-param>
      <param-name>pattern27</param-name>
      <param-value>.*/maps/(.+.)$</param-value>
    </init-param>
    <init-param>
      <param-name>template27</param-name>
      <param-value>/ViewMap.do?fileName=$1&amp;xsl=Gpx2Html.xsl&amp;height=600px&amp;width=auto</param-value>
    </init-param>
    <init-param>
      <param-name>pattern28</param-name>
      <param-value>.*/trailmaps</param-value>
    </init-param>
    <init-param>
      <param-name>template28</param-name>
      <param-value>/TrailheadMap.do</param-value>
    </init-param>
    <!-- Trackback Processing -->
    <init-param>
      <param-name>pattern29</param-name>
      <param-value>.*/trackback/(\d+)\??(.*)$</param-value>
    </init-param>
    <init-param>
      <param-name>template29</param-name>
      <param-value>/ReceiveTrackback.do?entryId=$1&amp;$2</param-value>
    </init-param>
    <!-- Testing Error Handling -->
    <init-param>
      <param-name>pattern30</param-name>
      <param-value>.*/throw$</param-value>
    </init-param>
    <init-param>
      <param-name>template30</param-name>
      <param-value>/ThrowAction.do</param-value>
    </init-param>
    <!-- Debugging -->
    <init-param>
      <param-name>debug</param-name>
      <param-value>false</param-value>
    </init-param>
